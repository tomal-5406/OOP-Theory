package slide4_exercises;

public class StudentUML {
	
	private String id;
	private String name;
	private double cgpa;

	public static void main(String[] args) {
		StudentUML std1=new StudentUML();
		
		std1.insert_Record("54554","name",3.98 );
		
		std1.display_Record();

	}
	
	public void insert_Record(String id,String name,double cgpa ) {
		
		this.id=id;
		this.name=name;
		this.cgpa=cgpa;
	}
	
	public void display_Record() {
		
		System.out.println("Student 1 Record :");
		System.out.println("ID="+id);
		System.out.println("Name: "+name);
		System.out.println("CGPA: "+cgpa);
		
	}

}
